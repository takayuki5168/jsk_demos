<launch>
  <node name="image_siftnode" pkg="imagesift" type="imagesift" output="screen" >
    <remap from="image" to="kinect_head/rgb/throttled/image_rect_color" />
    <remap from="camera_info" to="kinect_head/rgb/throttled/camera_info" />
  </node>

  <node name="point_pose_extractor" pkg="jsk_perception"
        type="point_pose_extractor" clear_params="true">
    <!-- <param name="template_filename" value="$(find jsk_2020_04_pr2_curry)/data/kitchen_mark.png"/> -->
    <!-- <param name="object_width" value="0.055" /> -->
    <!-- <param name="object_height" value="0.070" /> -->

    <!-- <param name="template_filename" value="$(find jsk_2020_04_pr2_curry)/data/kitchen_mark_2.png"/> -->
    <!-- <param name="object_width" value="0.055" /> -->
    <!-- <param name="object_height" value="0.070" /> -->

    <!-- <param name="template_filename" value="$(find jsk_2020_04_pr2_curry)/data/kitchen_mark_3.png"/> -->
    <!-- <param name="object_width" value="0.030" /> -->
    <!-- <param name="object_height" value="0.023" /> -->

    <param name="template_filename" value="$(find jsk_2020_04_pr2_curry)/data/kitchen_mark_4.png"/>
    <param name="object_width" value="0.055" />
    <param name="object_height" value="0.070" />

    <param name="reprojection_threshold" value="3.0" />  <!-- TODO cv::findHomography ransacReprojThreshold 許容される誤差の最大値 1から10が妥当10 3.0 -->
    <param name="distanceratio_threshold" value="0.49" /> <!-- TODO 0.6 0.49 -->
    <param name="relative_pose" value="0 0 0 0 0 0 1" />    <!-- TODO quaternion expression -->
    <param name="error_threshold" value="100.0" />
    <param name="window_name" value="microwave" />
    <!-- <param name="viewer_window" value="false" />  -->
  </node>
</launch>
